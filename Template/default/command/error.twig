{% extends template_file('@phpManufaktur/Basic/Template', 'kitcommand/iframe.body.twig') %}
{% block title %}{{ 'Event Error Message'|trans }}{% endblock %}
{% block description %}{{ 'Error message from the kitFramework Event application'|trans }}{% endblock %}
{% block stylesheet %}
  {{ parent() }}
  <link rel="stylesheet" type="text/css" href="{{ MANUFAKTUR_URL }}/Event/Template/default/command/css/command.css" media="screen, projection" />
{% endblock stylesheet %}
{% block content %}
  <div class="error_container">
    <h2>{{ 'Event: Error message'|trans }}</h2>
    <p>{{ error.trace|replace({"\n":"<br />"}) }}</p>
    {#{ dump(error) }#}
  </div>
{% endblock %}

